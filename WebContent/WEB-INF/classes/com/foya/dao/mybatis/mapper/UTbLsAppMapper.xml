  <select id="searchByCond" resultMap="BaseResultMap">
	SELECT   A.LS_NO,
			 A.APP_SEQ,
			 A.LS_VER,
             A.APP_STATUS,
             A.ADD_ITEM,
             A.ADD_TYPE,
             A.APP_USER,                     
             (
               SELECT Person.CHN_NM
                 FROM TB_AUTH_PERSONNEL Person
               WHERE Person.PSN_NO = M.APP_USER
             ) AS APP_USER_NAME,
             A.CR_TIME,
             A.LS_TYPE, 
             M.LS_STATUS AS LS_STATUS,
             M.DEAL_USER  AS DEAL_USER,
             (
                SELECT Person.CHN_NM
                  FROM TB_AUTH_PERSONNEL Person
                WHERE Person.PSN_NO = M.DEAL_USER
              ) AS DEAL_USER_NAME
	   FROM  TB_LS_APP AS  A
	         LEFT OUTER JOIN TB_LS_MAIN AS M ON A.LS_NO = M.LS_NO
	   WHERE 1=1
	   	<if test="expiredMonths != null and expiredMonths != ''" >
	     AND #{expiredMonths} >= DATEDIFF(MONTH, GETDATE(), M.LS_E_DATE)
	     AND DATEDIFF(MONTH, GETDATE(), M.LS_E_DATE) >= 0
	    </if>
	    <if test="lsNo != null and lsNo != ''" >
	     AND A.LS_NO LIKE #{lsNo}
	    </if>
	    <if test="locId != null and locId != ''" >
    	 AND  EXISTS ( 
						SELECT L.LOCATION_ID 
				  		  FROM TB_LS_LOCATION AS L
				         WHERE L.LOCATION_ID = #{locId}
				           AND  L.LS_NO = A.LS_NO
				           AND L.LS_VER = A.LS_VER
			           )
	    </if>
	    <if test="appUser != null and appUser != ''" >
	     AND A.APP_USER = #{appUser}
	    </if>
	    <if test="dealUser != null and dealUser != ''" >
	     AND M.DEAL_USER = #{dealUser}
	    </if>
	    <if test="opDomain != null and opDomain != ''" >
	     AND M.OP_DOMAIN LIKE #{opDomain}
	    </if>
	    <if test="lsType != null and lsType.length > 0">
	     	AND A.LS_TYPE IN	
		     <foreach collection="lsType" item="item" open="(" close=")" separator=",">
		      #{item}
		      </foreach>
	     </if>
	    <if test="appStatus != null and appStatus.length > 0">
	     	AND A.APP_STATUS IN	
		     <foreach collection="appStatus" item="item" open="(" close=")" separator=",">
		      #{item}
		      </foreach>
	     </if>
	    <if test="lsStatus != null and lsStatus.length > 0">
	     	AND M.LS_STATUS IN	
		     <foreach collection="lsStatus" item="item" open="(" close=")" separator=",">
		      #{item}
		      </foreach>
	     </if>	     	     
	    <if test="addItem != null and addItem != ''" >
	     AND A.ADD_ITEM = #{addItem}
	    </if>
  </select>
  
  <select id="selectMaxVerForTbLsApp" resultType="com.foya.noms.dto.ls.LeaseDTO" parameterType="String">
SELECT
	TEMP.LS_NO AS LS_NO,
	MAX (TEMP.LS_VER) AS LS_VER
FROM
		(
		SELECT
			LS_NO,
			CONVERT (INT, LS_VER) AS LS_VER
		FROM
			TB_LS_APP
		WHERE
			APP_SEQ = #{appSeq}
	) TEMP

GROUP BY
	TEMP.LS_NO
  </select>
</mapper>